#ifndef EX_SEMANTIC_H
#define EX_SEMANTIC_H

//
// Adds some meaning to function arguments (documentation only)
//

// example:
//    bool do_smth
//    (
//                   int         count,
//          NOT_NULL const char* name,
//          IN OUT   int&        data,
//          OPTIONAL const char* prefix = nullptr
//    );
//

#define IN
#define OUT
#define NOT_NULL
#define OPTIONAL


#endif // EX_SEMANTIC_H
